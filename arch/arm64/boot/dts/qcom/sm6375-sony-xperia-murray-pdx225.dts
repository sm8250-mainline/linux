/dts-v1/;

/* PMK8350 is configured to use SID6 instead of 0 */
#define PMK8350_SID 6

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/iio/qcom,spmi-adc7-pmk8350.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include "sm6375.dtsi"
#include "pm6125.dtsi"
#include "pmk8350.dtsi"
#include "pmr735a.dtsi"

/* PM6125 PON is used and we can't have duplicate labels */
/delete-node/ &pmk8350_pon;

/ {
	model = "Sony Xperia 10 IV";
	compatible = "sony,pdx225", "qcom,sm6375";
	chassis-type = "handset";

	aliases {
		bluetooth0 = &bluetooth;
		serial1 = &uart1;
	};

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		framebuffer: framebuffer@85200000 {
			compatible = "simple-framebuffer";
			reg = <0 0x85200000 0 0xc00000>;

			width = <1080>;
			height = <2520>;
			stride = <(1080 * 4)>;
			format = "a8r8g8b8";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-0 = <&vol_down_n>;
		pinctrl-names = "default";

		key-volume-down {
			label = "Volume Down";
			linux,code = <KEY_VOLUMEDOWN>;
			gpios = <&pmr735a_gpios 1 GPIO_ACTIVE_LOW>;
			debounce-interval = <15>;
			linux,can-disable;
			wakeup-source;
		};
	};

	reserved-memory {
		cont_splash_mem: memory@85200000 {
			reg = <0 0x85200000 0 0xc00000>;
			no-map;
		};

		ramoops@ffc40000 {
			compatible = "ramoops";
			reg = <0 0xffc40000 0 0xb0000>;
			record-size = <0x10000>;
			console-size = <0x60000>;
			ftrace-size = <0x10000>;
			pmsg-size = <0x20000>;
			ecc-size = <16>;
		};
	};

	display_avdd_regulator: display-panel-avdd-regulator {
		compatible = "regulator-fixed";
		regulator-name = "display_avdd_regulator";
		regulator-min-microvolt = <3000000>;
		regulator-max-microvolt = <3000000>;
		regulator-enable-ramp-delay = <233>;
		gpio = <&tlmm 24 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;//HACK
		regulator-boot-on;

		pinctrl-names = "default";
		pinctrl-0 = <&display_panel_avdd>;
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
		regulator-always-on;
		regulator-boot-on;
	};
};

&gpi_dma0 {
	status = "okay";
};

&gpi_dma1 {
	status = "okay";
};

&gpu {
	status = "okay";

	zap-shader {
		firmware-name = "qcom/sm6375/Sony/murray/a615_zap.mbn";
	};
};

&i2c8 {
	clock-frequency = <400000>;
	status = "okay";

	touchscreen@48 {
		// compatible = "samsung,s6sy761";
		reg = <0x48>;
		interrupt-parent = <&tlmm>;
		interrupts = <22 0x2008>;

		vdd-supply = <&pm6125_l13>;
		incell-supply = <&panel>;

		pinctrl-names = "default";
		pinctrl-0 = <&ts_int_default>;// &ts_avdd_default>;
	};
};

&mdss {
	status = "okay";
};

&mdss_dsi0 {
	vdda-supply = <&pm6125_l4>;
	status = "okay";

	panel: panel@0 {
		compatible = "samsung,amoled";
		reg = <0>;

		// disp-te-gpios = <&tlmm 23 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&pmr735a_gpios 2 GPIO_ACTIVE_LOW>;

		// avdd-supply = <&display_avdd_regulator>;
		// vdd-supply = <&pm6125_l8>;

		pinctrl-0 = <&panel_reset_pin &te_pin_default &ts_avdd_default>;
		pinctrl-names = "default";

		#address-cells = <1>;
		#size-cells = <0>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&mdss_dsi0_out>;
			};
		};
	};
};

&mdss_dsi0_out {
	remote-endpoint = <&panel_in>;
	data-lanes = <0 1 2 3>;
};

&mdss_dsi0_phy {
	status = "okay";
};

&pm6125_adc {
	/* Looks like only PMK8350 shows correct XO temp values */
	/delete-node/ xo-therm@4c;

	pa1-therm@4d {
		reg = <ADC5_AMUX_THM1_100K_PU>;
		label = "pa1_therm";
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	quiet-therm@4e {
		reg = <ADC5_AMUX_THM2_100K_PU>;
		label = "quiet_therm";
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	msm-therm@52 {
		reg = <ADC5_GPIO1_100K_PU>;
		label = "msm_therm";
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	/* Readouts will be wrong, ODM messed something up again and used a board-specific LUT.. */
	batt-therm@53 {
		reg = <ADC5_GPIO2_100K_PU>;
		label = "batt_therm";
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	/* Bogus readouts, this should be a voltage and not a thermal channel.. */
	mb-stageid@54 {
		reg = <ADC5_GPIO3_100K_PU>;
		label = "mb_stageid";
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	pa2-therm@55 {
		reg = <ADC5_GPIO4_100K_PU>;
		label = "pa2_therm";
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};
};

&pm6125_adc_tm {
	status = "okay";

	quiet-therm@0 {
		reg = <0>;
		io-channels = <&pm6125_adc ADC5_AMUX_THM2_100K_PU>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	mb-stageid@1 {
		reg = <1>;
		io-channels = <&pm6125_adc ADC5_GPIO3_100K_PU>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	pa-therm1@2 {
		reg = <2>;
		io-channels = <&pm6125_adc ADC5_AMUX_THM1_100K_PU>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	pa-therm2@3 {
		reg = <3>;
		io-channels = <&pm6125_adc ADC5_GPIO4_100K_PU>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

&pmk8350_adc_tm {
	status = "okay";

	sys-therm@0 {
		reg = <0>;
		io-channels = <&pmk8350_vadc PMK8350_ADC7_AMUX_THM1_100K_PU>;
		qcom,hw-settle-time-us = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	sys-therm@1 {
		reg = <1>;
		io-channels = <&pmk8350_vadc PMK8350_ADC7_AMUX_THM4_100K_PU>;
		qcom,hw-settle-time-us = <200>;
		qcom,ratiometric;
	};
};

&pmk8350_rtc {
	status = "okay";
};

&pmk8350_vadc  {
	status = "okay";

	ref-gnd@600 {
		reg = <PMK8350_ADC7_REF_GND>;
		label = "ref_gnd";
		qcom,pre-scaling = <1 1>;
	};

	ref-1p25@601 {
		reg = <PMK8350_ADC7_1P25VREF>;
		label = "vref_1p25";
		qcom,pre-scaling = <1 1>;
	};

	die-temp@603 {
		reg = <PMK8350_ADC7_DIE_TEMP>;
		label = "die_temp";
		qcom,pre-scaling = <1 1>;
	};

	xo-therm@644 {
		reg = <PMK8350_ADC7_AMUX_THM1_100K_PU>;
		label = "xo_therm";
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
	};

	/* Readouts will be wrong, ODM messed something up again and used a board-specific LUT.. */
	conn-therm@647 {
		reg = <PMK8350_ADC7_AMUX_THM4_100K_PU>;
		label = "conn_therm";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

&pmr735a_gpios {
	vol_down_n: vol-down-n-state {
		pins = "gpio1";
		function = "normal";
		power-source = <1>;
		bias-pull-up;
		input-enable;
	};

	panel_reset_pin: panel-reset-state {
		pins = "gpio2";
		function = "func1";
		qcom,drive-strength = <PMIC_GPIO_STRENGTH_HIGH>;
		power-source = <1>;
		bias-disable;
		output-low;
	};
};

&pon_pwrkey {
	status = "okay";
};

&pon_resin {
	linux,code = <KEY_VOLUMEUP>;
	status = "okay";
};

&qupv3_id_0 {
	status = "okay";
};

&qupv3_id_1 {
	status = "okay";
};

&remoteproc_adsp {
	firmware-name = "qcom/sm6375/Sony/murray/adsp.mbn";
	status = "okay";
};

&remoteproc_cdsp {
	firmware-name = "qcom/sm6375/Sony/murray/cdsp.mbn";
	status = "okay";
};

&remoteproc_mss {
	firmware-name = "qcom/sm6375/Sony/murray/modem.mbn";
	status = "okay";
};

&rpm_requests {
	regulators-0 {
		compatible = "qcom,rpm-pm6125-regulators";

		pm6125_s5: s5 {
			/* Originally 0.382V-1.12V but BT expects 0.984V */
			regulator-min-microvolt = <984000>;
			regulator-max-microvolt = <984000>;
			regulator-always-on;//HACK
		};

		pm6125_s6: s6 {
			regulator-min-microvolt = <320000>;
			regulator-max-microvolt = <1374000>;
		};

		pm6125_s7: s7 {
			regulator-min-microvolt = <1574000>;
			regulator-max-microvolt = <2040000>;
		};

		/*
		 * S8 is VDD_GFX
		 * L1 is VDD_LPI_CX
		 */

		pm6125_l2: l2 {
			/* Originally 1.17V-1.304V but BT expects 1.304V */
			regulator-min-microvolt = <1304000>;
			regulator-max-microvolt = <1304000>;
		};

		pm6125_l3: l3 {
			regulator-min-microvolt = <1100000>;
			regulator-max-microvolt = <1300000>;
		};

		pm6125_l4: l4 {
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
			regulator-allow-set-load;
		};

		pm6125_l5: l5 {
			regulator-min-microvolt = <1650000>;
			regulator-max-microvolt = <2960000>;
			regulator-allow-set-load;
		};

		pm6125_l6: l6 {
			regulator-min-microvolt = <1080000>;
			regulator-max-microvolt = <1304000>;
		};

		pm6125_l7: l7 {
			regulator-min-microvolt = <720000>;
			regulator-max-microvolt = <1050000>;
		};

		pm6125_l8: l8 {
			regulator-min-microvolt = <1100000>;
			regulator-max-microvolt = <1304000>;
			regulator-always-on;
		};

		pm6125_l9: l9 {
			/* Originally 1.504V-2V but BT expects 1.7-1.9V */
			regulator-min-microvolt = <1700000>;
			regulator-max-microvolt = <1900000>;
		};

		pm6125_l10: l10 {
			regulator-min-microvolt = <1620000>;
			regulator-max-microvolt = <1980000>;
		};

		pm6125_l11: l11 {
			regulator-min-microvolt = <1620000>;
			regulator-max-microvolt = <1980000>;
		};

		pm6125_l12: l12 {
			regulator-min-microvolt = <1620000>;
			regulator-max-microvolt = <2000000>;
		};

		pm6125_l13: l13 {
			regulator-min-microvolt = <1650000>;
			regulator-max-microvolt = <1980000>;
			regulator-always-on;
		};

		pm6125_l14: l14 {
			regulator-min-microvolt = <1700000>;
			regulator-max-microvolt = <1900000>;
		};

		pm6125_l15: l15 {
			regulator-min-microvolt = <1650000>;
			regulator-max-microvolt = <3544000>;
		};

		pm6125_l16: l16 {
			/* Originally 1.62V-1.98V but BT expects 1.7-1.9V */
			regulator-min-microvolt = <1700000>;
			regulator-max-microvolt = <1900000>;
		};

		/* L17 is VDD_LPI_MX */

		pm6125_l18: l18 {
			regulator-min-microvolt = <830000>;
			regulator-max-microvolt = <920000>;
		};

		pm6125_l19: l19 {
			regulator-min-microvolt = <1624000>;
			regulator-max-microvolt = <3304000>;
		};

		pm6125_l20: l20 {
			regulator-min-microvolt = <1624000>;
			regulator-max-microvolt = <3304000>;
		};

		pm6125_l21: l21 {
			/* Originally 3.0V-3.4V but Wi-Fi expects 3.0-3.312V */
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3312000>;
		};

		pm6125_l22: l22 {
			regulator-min-microvolt = <2704000>;
			regulator-max-microvolt = <2960000>;
			regulator-allow-set-load;
		};

		pm6125_l23: l23 {
			/* Originally 3.0V-3.4V but Wi-Fi expects 3.0-3.312V */
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3312000>;
		};

		pm6125_l24: l24 {
			regulator-min-microvolt = <2704000>;
			regulator-max-microvolt = <3544000>;
		};
	};

	regulators-1 {
		compatible = "qcom,rpm-pmr735a-regulators";

		/*
		 * S1 is VDD_MX
		 * S2 is VDD_CX
		 */

		pmr735a_l1: l1 {
			regulator-min-microvolt = <570000>;
			regulator-max-microvolt = <650000>;
		};

		pmr735a_l2: l2 {
			/* Originally 0.352V-0.796V but Wi-Fi expects 0.64V */
			regulator-min-microvolt = <640000>;
			regulator-max-microvolt = <640000>;
		};

		pmr735a_l3: l3 {
			regulator-min-microvolt = <1000000>;
			regulator-max-microvolt = <1200000>;
		};

		pmr735a_l4: l4 {
			regulator-min-microvolt = <1504000>;
			regulator-max-microvolt = <2000000>;
		};

		pmr735a_l5: l5 {
			regulator-min-microvolt = <751000>;
			regulator-max-microvolt = <824000>;
		};

		pmr735a_l6: l6 {
			regulator-min-microvolt = <504000>;
			regulator-max-microvolt = <868000>;
		};

		pmr735a_l7: l7 {
			regulator-min-microvolt = <2700000>;
			regulator-max-microvolt = <3544000>;
		};
	};
};

&sdc2_off_state {
	sd-cd-pins {
		pins = "gpio94";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
	};
};

&sdc2_on_state {
	sd-cd-pins {
		pins = "gpio94";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-up;
	};
};

&sdhc_2 {
	status = "okay";

	vmmc-supply = <&pm6125_l22>;
	vqmmc-supply = <&pm6125_l5>;

	cd-gpios = <&tlmm 94 GPIO_ACTIVE_HIGH>;
};

&tlmm {
	gpio-reserved-ranges = <13 4>;

	ts_int_default: ts-int-default-state {
		pins = "gpio22";
		function = "gpio";
		drive-strength = <8>;
		bias-pull-up;
	};

	te_pin_default: te-pin-default {
		pins = "gpio23";
		function = "mdp_vsync";
		drive-strength = <2>;
		bias-pull-down;
	};

	display_panel_avdd: display-panel-avdd-state {
		pins = "gpio24";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	ts_avdd_default: ts-avdd-default-state {
		pins = "gpio59";
		function = "gpio";
		drive-strength = <8>;
		output-high;
	};

	sw_ctrl: sw-ctrl-state {
		pins = "gpio69";
		function = "gpio";
	};
};

&uart1 {
	/delete-property/ interrupts;
	interrupts-extended = <&intc GIC_SPI 328 IRQ_TYPE_LEVEL_HIGH>,
			      <&tlmm 64 IRQ_TYPE_LEVEL_HIGH>;
	// status = "okay";

	bluetooth: bluetooth {
		compatible = "qcom,wcn3990-bt";
		swctrl-gpios = <&tlmm 69 GPIO_ACTIVE_HIGH>;
		pinctrl-0 = <&sw_ctrl>;
		pinctrl-names = "default";

		vddio-supply = <&pm6125_l9>;
		vddxo-supply = <&pm6125_l16>;
		vddrf-supply = <&pm6125_l2>;
		vddch0-supply = <&pm6125_l23>;
		vddsmps-supply = <&pm6125_s5>;
		max-speed = <3200000>;
	};
};

&usb_1 {
	status = "okay";
};

&usb_1_dwc3 {
	dr_mode = "peripheral";
};

&usb_1_hsphy {
	status = "okay";
};

&wifi {
	vdd-0.8-cx-mx-supply = <&pmr735a_l2>;
	vdd-1.8-xo-supply = <&pm6125_l16>;
	vdd-1.3-rfa-supply = <&pm6125_l2>;
	vdd-3.3-ch0-supply = <&pm6125_l23>;
	vdd-3.3-ch1-supply = <&pm6125_l21>;
	vdd-smps-supply = <&pm6125_s5>;
	status = "okay";
};

&xo_board_clk {
	clock-frequency = <19200000>;
};
